%
% internal sequence of commands for including a PDF solution at end of document
% 
\def\ueb@solattached@AtEndDocumentCommands#1#2#3#4{%
  \def\ueb@solattached@labelforthisexercise{#1}%
  \def\ueb@solattached@labelforthisexercisepart{#2}
  \def\ueb@solattached@pdftoinclude{#3}%
  \def\ueb@solattached@pdfincludeoptions{#4}%
  \def\uebattachedsolutionexercisenumber{\expandafter\ref{\ueb@solattached@labelforthisexercise}}%
  \setlength\unitlength{1pt}%
  \includepdf[pagecommand={%
      \ueb@AttachSolutionIncludepdfPageCommand%
    },%
    picturecommand*={%
      % add the label to the first page of our solutions
      \refstepcounter{uebattachsoldummy}\expandafter\label{solattached:\ueb@solattached@labelforthisexercisepart}%
      % add any custom overlay stuff to the first page only
      \ueb@AttachSolutionIncludepdfPictureCommandStar%
    }, %
    picturecommand={%
      % add overlay stuff to each included page; this is by default our title
      \ueb@AttachSolutionIncludepdfPictureCommand%
    }, #4]% those where the options to \includepdf[]{}
  {\ueb@solattached@pdftoinclude}% the pdf file to include
}%
